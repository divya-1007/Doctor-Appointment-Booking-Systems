<script>
     $(document).ready(function () {
             //Common form request
            $("form[name=ajax_form]").submit(function (e) {
                var formData = new FormData(this);
            var token =  $(this).find("input[name=token]").val();
            e.preventDefault();
            $.ajax({
                    type: 'POST',
                    url:$(this).attr('action'),
                    data: formData,
                    cache: false,
                    contentType: false,
                    processData: false,
                    headers:{ Authorization: 'Bearer '+ token},
                    success: function (response) {
                      if(response.status==200){
                        swal({
                        title: "Success......✔️",
                        text: response.message,
                        icon: "success",
                        button: "Ok..!",
                        });
                        //location.reload();
                      }else{
                          swal(
                          'Oops...',
                          response.message,
                          'error'
                          );
                      }
                    }
                });
            });
//Change password form request
        $("#change-password-form").submit(function (e) {
        e.preventDefault();
        var fromData = $(this).serialize();
        var token =  $(this).find("input[name=token]").val(); 
         $.ajax({
                type: 'POST',
               url:$(this).attr('action'),
                data:fromData,
                headers:{ Authorization: 'Bearer '+ token},
                success: function (response) {
                  if(response.status==200){
                    swal({
                    title: "Success......✔️",
                    text: response.message,
                    icon: "success",
                    button: "Ok...!",
                    });
                  }else{
                      swal(
                      'Oops...',
                      response.message,
                      'error'
                      );
                  }
                }
           });
         });

         //delete-account form request
       $("#delete-account").submit(function (e) {
         e.preventDefault();
         var fromData = $(this).serialize();
         var token = $(this).find("input[name=token]").val();
         var id = $(this).find("input[name=_id]").val();
         $.ajax({
           type: $(this).attr('method'),
           url: $(this).attr('action'),
           data: fromData,
           headers: { Authorization: 'Bearer ' + token },
           success: function (response) {
              if(response.status==200){
                swal({
                title: "Success....✔️",
                text: response.message,
                icon: "success",
                button: "Ok..!",
                }).then(function() {
                  window.location.href = '/SignIn'
                });
            }else{
                swal(
                'Oops...',
                response.message,
                'error'
                );
            }
           }
         });
       });
     });
    
        </script>
        